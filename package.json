{
  "name": "adsb-receiver-api",
  "version": "0.5.0",
  "description": "Repository contains an ADS-B receiver API that receives data from virtual, flight-simulator-based sources.",
  "author": "oskarbarcz",
  "private": true,
  "license": "proprietary",
  "scripts": {
    "build": "nest build",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest",
    "test:functional": "npx cucumber-js --require ./features/_context/**/*.ts --require-module ts-node/register"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@types/uuid": "^9.0.8",
    "cache-manager": "^7.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "helmet": "^8.1.0",
    "keyv": "^5.5.0",
    "rxjs": "^7.8.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.22.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.0.0",
    "axios": "^1.11.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-gherkin": "^3.1.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  }
}
